
@{
    ViewData["Title"] = "Customer DashBoard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

@model BuildCompleteEcommerce.Models.Account

<div class="span9">
    <ul class="breadcrumb">
        <li><a asp-controller="home" asp-action="index">Home</a> <span class="divider">/</span></li>
        <li class="active">DashBoard</li>
    </ul>
    <h3>Customer DashBoard</h3>
    <div class="well">

        <h4>
            <a asp-controller="customer" asp-action="profile">Change Profile</a>
        </h4>
        <hr class="soft" />

        <h4>
            <a asp-controller="customer" asp-action="history">Invoices Management</a>
        </h4>
        <hr class="soft" />

        <h4>
            <a asp-controller="customer" asp-action="signout">Logout</a>
        </h4>
        <hr class="soft" />


    </div>

</div>

